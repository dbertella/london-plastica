{
  "name": "gatsby-starter-netlify-cms",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "1.1.3",
  "author": "Austin Green",
  "dependencies": {
    "bulma": "^0.7.0",
    "date-fns": "^1.30.1",
    "faunadb": "^2.6.0",
    "gatsby": "^2.0.66",
    "gatsby-image": "^2.0.23",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^3.0.9",
    "gatsby-plugin-purgecss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-typescript": "^2.0.9",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "^2.4.2",
    "netlify-identity-widget": "^1.5.2",
    "netlify-lambda": "^1.4.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.9.4",
    "prop-types": "^15.6.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "uuid": "^3.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "npm-run-all --parallel start:app start:server",
    "start:app": "npm run develop",
    "start:server": "netlify-lambda serve functions -c ./webpack.config.js",
    "clean": "rimraf .cache public",
    "build": "npm-run-all --parallel build:**",
    "build:app": "npm run clean && gatsby build",
    "build:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "develop": "npm run clean && gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.121",
    "@types/netlify-identity-widget": "^1.4.1",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.10",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby-plugin-manifest": "^2.0.19",
    "gatsby-plugin-styled-components": "^3.0.6",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "typescript": "^3.3.3333"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
